<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".adminpanel.AdminHomePanelActivity">


    <TextView
        android:id="@+id/adminResponseWarningId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="This is admin panel"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/adminLogoutButtonId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="Logout"
        app:layout_constraintEnd_toEndOf="@+id/adminResponseWarningId"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="@+id/adminResponseWarningId"
        app:layout_constraintTop_toBottomOf="@+id/adminResponseWarningId" />
</androidx.constraintlayout.widget.ConstraintLayout>